<p id="notice"><%= notice %></p>

<h1>Listing Webhosts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Attackid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webhosts.each do |webhost| %>
      <tr>
        <td><%= webhost.name %></td>
        <td><%= webhost.attackID %></td>
        <td><%= link_to 'Show', webhost %></td>
        <td><%= link_to 'Edit', edit_webhost_path(webhost) %></td>
        <td><%= link_to 'Destroy', webhost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Webhost', new_webhost_path %>
